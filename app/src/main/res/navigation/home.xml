<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.onix.internship.ui.splash.SplashFragment"
        android:label="SplashFragment" >
        <action
            android:id="@+id/action_splashFragment_to_customCameraActivity"
            app:destination="@id/customCameraActivity" />
    </fragment>
    <activity
        android:id="@+id/customCameraActivity"
        android:name="com.onix.internship.ui.customCamera.CustomCameraActivity"
        android:label="CustomCameraActivity" />

</navigation>